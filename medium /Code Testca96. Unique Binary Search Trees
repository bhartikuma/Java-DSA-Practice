96. Unique Binary Search Trees
class Solution {
    public int numTrees(int n) {
        int[] dp = new int[n + 1];
        dp[0] = 1;  
        dp[1] = 1;  
        
        for (int nodes = 2; nodes <= n; nodes++) {
            for (int root = 1; root <= nodes; root++) {
                int leftTrees = dp[root - 1];
                int rightTrees = dp[nodes - root];
                dp[nodes] += leftTrees * rightTrees;
            }
        }
        return dp[n];
    }
}
